/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "esModuleInterop": true,
    "target": "ES2022",
    "module": "ES2022",
    "lib": [
      "ES2022",
      "dom"
    ],
    "strict": false,
    "paths": {
      "#core/*": [
        "src/app/core/*"
      ]
    }
  },
  "exclude": ["cypress"],
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false
  }
}
